<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.myfpm">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/truelogo"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:screenOrientation="portrait"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".MyFPMpage"
                android:screenOrientation="portrait"
                android:label="@string/title_activity_my_fpmpage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name=".New_news"
                  android:screenOrientation="portrait">

            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".MyFPMpage">

            </meta-data>
        </activity>
        <activity android:name=".ResetPasswordActivity"
                  android:screenOrientation="portrait">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".LoginActivity"/>
        </activity>
        <activity
                android:name=".RegPersonPage"
                android:screenOrientation="portrait"
                android:label="Регистрация. Шаг 2/3">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".Reg1Page"/>
        </activity>
        <activity
                android:name=".Reg2Page"
                android:screenOrientation="portrait"
                android:label="Регистрация. Шаг 3/3">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".RegPersonPage"/>
        </activity>
        <activity
                android:name=".Reg1Page"
                android:screenOrientation="portrait"
                android:label=" Регистрация. Шаг 1/3">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value=".LoginActivity"/>
        </activity>
        <activity android:name=".LoginActivity"
                  android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>